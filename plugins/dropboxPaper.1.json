{
    "priority": "10",
    "name": "dropbox-paper",
    "urlMatcher": ".*paper.dropbox.com\\/doc\\/.*",
    "subjectTemplate": "$('#editor-1-ace-line-1')[0].innerText",
    "bodyTemplateFile": "plugins/dropboxPaperTemplate.html",
    "searchQuery": "url = document.location.href; url.slice(url.lastIndexOf('/')+1, url.lastIndexOf('-')).replace(RegExp(`-`,`g`),` `)"
}